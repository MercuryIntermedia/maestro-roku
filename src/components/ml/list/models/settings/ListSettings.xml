<?xml version='1.0' encoding='utf-8'?>
<component
    name='ml_ListSettings'
    extends='ContentNode'>
    <interface>
      <field id='lookAheadDistance' type='integer' value='5000'/>
      <field id='lookBehindDistance' type='integer' value='2000'/>
      <field id='sendViewPortEvents' type='boolean' value='false'/>

      <!-- async rendering support -->
      <!-- all|async -->
      <field id='rowCreationStrategy' type='string' value='all'/>
      <field id='rowCreationBatchSize' type='integer' value='3'/>
      <field id='rowCreationBatchDelay' type='float' value='0.15'/>
    </interface>
</component>